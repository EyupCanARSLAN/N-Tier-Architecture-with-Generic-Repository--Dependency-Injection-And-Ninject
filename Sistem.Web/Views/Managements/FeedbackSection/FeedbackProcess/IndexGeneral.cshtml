@model IEnumerable<Sistem.Core.Data.CritiqK.GeneralCritiq>
@{
    ViewBag.Title = "General Feedback By Customer";
}
<h2>General Feedback By Customer</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(Model=>Model.CustomerId)
        </th>
     
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerId)
        </td>
        <td>
            @Html.ActionLink("See The Feedback", "DetailsAboutGeneralFeedback", new { FeedbackId = item.GeneralCritiqId}) |
        </td>
    </tr>
}
</table>
@Html.ActionLink("See The General Feedback", "Index", new { Type = 1 }) |
@Html.ActionLink("See The Menu Feedback", "Index", new { Type = 2 }) |
@Html.ActionLink("See The Order Feedback", "Index", new { Type = 3 }) |